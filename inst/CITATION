citHeader("To cite the 'MOGAMUN' package in publications use:")

year <- sub(".*(2[[:digit:]]{3})-.*", "\\1", meta$Date, perl = TRUE)
vers <- paste("R package version", meta$Version)

citEntry(
    entry="misc",
    title = "A Multi-Objective Genetic Algorithm to Find Active Modules in Multiplex Biological Networks",
    author = 
        personList(
            as.person("Elva-María Novoa-del-Toro"),
            as.person("Efrén Mezura-Montes"), 
            as.person("Matthieu Vignes"),
            as.person("Morgane Térézol"),
            as.person("Frédérique Magdinier,"),
            as.person("Laurent Tichit"),
            as.person("Anaïs Baudot")
        ),
    year = "2021",
    note = 
        paste(
            "Manuscript available in",
            "https://journals.plos.org/ploscompbiol/article?id=10.1371/journal.pcbi.1009263"),
    textVersion =
        paste("Elva-María Novoa-del-Toro, Efrén Mezura-Montes, Matthieu Vignes, Morgane Térézol, Frédérique Magdinier, Laurent Tichit, and Anaïs Baudot:", 
            "A Multi-Objective Genetic Algorithm to Find Active Modules in",
            "Multiplex Biological Networks.",
            "https://journals.plos.org/ploscompbiol/article?id=10.1371/journal.pcbi.1009263",
            "(2021).", sep = " "
        )
)
